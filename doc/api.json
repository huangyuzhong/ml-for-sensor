			}
		},
		"/locations": {
			"method": "GET",
			"docstring": "get child locations given parent location id, if parent location id is null, use login user's company id to get all top leve locations",
			"request_schema": {
				"properties": {
					"parent_id": {
						"type": "integer"
					}
				}
			},
			"reply_schema": {
				"type": "array",
				"array_type": "RestLocation"
			}
		},
		"/location/devices": {
			"method": "GET",
			"docstring": "used to be /devices , get devices of a leaf location given location id",
			"request_schema": {
				"properties": {
					"location_id": {
						"type": "integer"
					}
				},
				"required": ["location_id"]
			},
