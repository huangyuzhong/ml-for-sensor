	"base_path": "/api/rest",
	"apis": {

		"/device/current/data": {
			"method": "GET",
			"docstring": "get monitoring data of a device from specified time till now",
			"request_schema": {
				"properties": {
					"deviceId": {
						"type": "integer"
					},
					"timeVal": {
						"type": "integer"
					}
				},
				"required": ["deviceId"]
			},
			"reply_schema": {
				"type": "object",
				"properties":{
					"running_status": {
						"type": "integer"
					},
					"tsData": {
						"type": "RestDeviceMonitoringTSData"

					}
				}
			}
		},

		"/socket/insert/data": {
			"method": "POST",
			"docstring": "添加监控报文",
			"request_schema": {
